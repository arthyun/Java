<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hyun.board.main.repository.MainRepository">
    <select id="getUsers" resultType="MainResponseDto">
        SELECT id, username, AES_DECRYPT(password, 'encryption_key') AS password, email, nickname FROM users;
    </select>
</mapper>